package com.registration;

import java.util.ArrayList;
import java.util.function.Predicate;

import com.lambda.Employee;

public class DataHandler {

	public static void main(String[] args) {
		ArrayList<Registration> register = new ArrayList<>();
		register.add(new Registration(101,"Rajesh","Sharma","2045G,Sethienclave","8107142274","rajshrma@gmail.com"));
		RegHandler regHandler = new RegHandler();
		
		Predicate<Registration> cond1 = r->regHandler.allValid(r);
		
		register.stream().filter(cond1).forEach(Registration::toString);
		
	}

}
